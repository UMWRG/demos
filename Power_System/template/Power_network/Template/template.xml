<?xml version="1.0"?>
<template_definition>
<template_name>Power_network</template_name>
<layout>    
        <item>
            <name>grouping</name>
            <value>			    
			    <name>Power_network</name>
				<description>Power network</description>
				<categories>
				    <category>
						<name>Resources</name>
						<description>Network resources</description>
						<displayname>Network resources</displayname>
						<groups>
							<group>
								<name>consumers</name>
								<description>consumers</description>
								<displayname>consumers</displayname>
								<image>images\consumer.png</image>
							</group>  
							<group>
								<name>junctions</name>
								<description>junctions</description>
								<displayname>junctions</displayname>
								<image>images\junction_2.png</image>
							</group> 														
							<group>
								<name>generators</name>
								<description>generators</description>
								<displayname>generators</displayname>
								<image>images\generators.png</image>
							</group>
							<group>
								<name>variable_flow</name>
								<description>variable_flow</description>
								<displayname>variable_flow</displayname>
								<image>images\link.png</image>
							</group>	
							<group>
								<name>Fixed_flow</name>
								<description>Fixed_flow</description>
								<displayname>Fixed_flow</displayname>
								<image>images\link.png</image>
							</group>								
						</groups>
				    </category>	
				</categories>
            </value>
        </item>
    </layout>
<resources>    
    <resource>
        <type>NETWORK</type>
        <name>Power_network</name>		       
    </resource>      
    <resource>
          <type>NODE</type>
        <name>consumers</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\consumer.png</value>
            </item>
            <item>
                <name>group</name>
                <value>consumers</value>
            </item>
        </layout>   
	    <attribute>
            <name>power_consumption</name>
            <dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
			<data_type>scalar</data_type>
        </attribute>
	<attribute>
            <name>fc</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>
        </attribute>		
    </resource>   
    <resource>
          <type>NODE</type>
        <name>generators</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\generator.png</value>
            </item>
            <item>
                <name>group</name>
                <value>generators</value>
            </item>
        </layout>   
	    <attribute>
            <name>power_generation</name>
            <dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
			<data_type>scalar</data_type>
        </attribute>	
		<attribute>
            <name>max_genration</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>
        </attribute>
		<attribute>
            <name>min_genration</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>
        </attribute>       	
    </resource>   
   <resource>
        <type>NODE</type>
        <name>junctions</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\junction_2.png</value>
            </item>
            <item>
                <name>group</name>
                <value>junctions</value>
            </item>
        </layout>          	
    </resource> 
	<resource>
        <type>LINK</type>
        <name>variable_flow</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\link.png</value>
            </item>
			<item>
					<name>symbol</name>
					<value>solid</value>
				</item>
				<item>
					<name>colour</name>
					<value>black</value>
				</item>
				<item>
					<name>line_weight</name>
					<value>2</value>
				</item>   
            <item>
                <name>group</name>
                <value>variable_flow</value>
            </item>	
			</layout>
			<attribute>
            <name>power_flow</name>
            <dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
			<data_type>scalar</data_type>
        </attribute>    
	</resource> 
    <resource>
        <type>LINK</type>
        <name>Fixed_flow</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\link.png</value>
            </item>
			<item>
					<name>symbol</name>
					<value>solid</value>
				</item>
				<item>
					<name>colour</name>
					<value>black</value>
				</item>
				<item>
					<name>line_weight</name>
					<value>2</value>
				</item>   
            <item>
                <name>group</name>
                <value>Fixed_flow</value>
            </item>
        </layout>	
		<attribute>
            <name>fixed_power_flow</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>
        </attribute>				
    </resource>     
	<resource>
        <type>GROUP</type>
        <name>mrf</name>
		<layout>
            <item>
                <name>image</name>
                <value>images\mrf.png</value>
            </item>
        </layout>	
    </resource>   	
  </resources>
</template_definition>
