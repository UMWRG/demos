<?xml version="1.0"?>
<template_definition>
<template_name>SiouxFalls</template_name>
<layout>    
        <item>
            <name>grouping</name>
            <value>			    
			    <name>SiouxFalls</name>
				<description>Traffic for SiouxFalls network.</description>
				<categories>
				    <category>
						<name>Resources</name>
						<description>Network resources</description>
						<displayname>Network resources</displayname>
						<groups>
							<group>
								<name>city</name>
								<description>city</description>
								<displayname>city</displayname>
								<image>images\city_4.png</image>
							</group>                             
							<group>
								<name>path</name>
								<description>path between two cities</description>
								<displayname>path</displayname>
								<image>images\red_line.png</image>
							</group>	
						</groups>
				    </category>	
				</categories>
            </value>
        </item>
    </layout>
<resources>    
    <resource>
        <type>NETWORK</type>
        <name>SiouxFalls_traffic</name>		       
		<attribute>
            <name>objective</name>
            <dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
			<data_type>scalar</data_type>
        </attribute>					
    </resource>      
    <resource>
        <type>NODE</type>
        <name>city</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\city_4.png</value>
            </item>
            <item>
                <name>group</name>
                <value>city</value>
            </item>
        </layout>      
        <attribute>
            <name>trip</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>array</data_type>
        </attribute>
		<attribute>
            <name>t</name>
            <dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
			<data_type>array</data_type>
        </attribute>		
    </resource>       	
    <resource>
        <type>LINK</type>
        <name>path</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\red_line.png</value>
            </item>
			<item>
					<name>symbol</name>
					<value>solid</value>
				</item>
				<item>
					<name>colour</name>
					<value>black</value>
				</item>
				<item>
					<name>line_weight</name>
					<value>1</value>
				</item>   
            <item>
                <name>group</name>
                <value>path</value>
            </item>
        </layout>
		<attribute>
            <name>a</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>
        </attribute>
		<attribute>
            <name>b</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>
        </attribute>
		<attribute>
            <name>c</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>
        </attribute>       
		<attribute>
            <name>v</name>
            <dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
			<data_type>scalar</data_type>
        </attribute>			
    </resource>        	
</resources>
</template_definition>
