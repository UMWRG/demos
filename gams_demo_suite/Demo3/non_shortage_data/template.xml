<?xml version="1.0"?>
<template_definition>
<template_name>GAMS App tutorial template</template_name>
<resources>
    
    <resource>
        <type>NETWORK</type>
        <name>tutorial_network</name>
        <attribute>
            <name>Z</name>
            <dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
        </attribute>
    </resource>
  
    <resource>
        <type>NODE</type>
        <name>supply</name>
        <attribute>
            <name>inflow</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
            <data_type>timeseries</data_type>
        </attribute>
        <attribute>
            <name>min_storage</name>
            <dimension>Volume</dimension>
            <is_var>N</is_var>
        </attribute>
        <attribute>
            <name>max_storage</name>
            <dimension>Volume</dimension>
            <is_var>N</is_var>
        </attribute>
        <attribute>
            <name>initial_storage</name>
            <dimension>Volume</dimension>
            <is_var>N</is_var>
        </attribute>
        <attribute>
            <name>S</name>
            <dimension>Volume</dimension>
            <is_var>Y</is_var>
        </attribute>
    </resource>
  
    <resource>
        <type>NODE</type>
        <name>non_storage</name>
        <attribute>
            <name>cost</name>
            <dimension>Monetary value</dimension>
            <is_var>N</is_var>
            </attribute>
        <attribute>
            <name>demand</name>
            <dimension>Volume</dimension>
            <is_var>N</is_var>
        </attribute>
    </resource>

    <resource>
        <type>LINK</type>
        <name>link</name>
        <attribute>
            <name>Q</name>
            <dimension>Volumetric flow rate</dimension>
            <is_var>Y</is_var>
        </attribute>
        <attribute>
            <name>cost</name>
            <dimension>Monetary value</dimension>
            <is_var>N</is_var>
        </attribute>
        <attribute>
            <name>flow_multiplier</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
        </attribute>
        <attribute>
            <name>min_flow</name>
            <dimension>Volumetric flow rate</dimension>
            <is_var>N</is_var>
        </attribute>
        <attribute>
            <name>max_flow</name>
            <dimension>Volumetric flow rate</dimension>
            <is_var>N</is_var>
        </attribute>
    </resource>

    <resource>
        <type>GROUP</type>
        <name>group</name>
    </resource>
</resources>
</template_definition>
