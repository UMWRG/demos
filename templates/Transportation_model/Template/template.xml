<?xml version="1.0"?>
<template_definition>
<template_name>transportation_model</template_name>
<layout>    
        <item>
            <name>grouping</name>
            <value>			    
			    <name>transportation_model</name>
				<description>Transportation example, the cost of the transportation is to be minimized given supply and demand constraints.</description>
				<categories>
				    <category>
						<name>Resources</name>
						<description>Network resources</description>
						<displayname>Network resources</displayname>
						<groups>
							<group>
								<name>city</name>
								<description>city</description>
								<displayname>city</displayname>
								<image>images\reservoir.png</image>
							</group>
                            <group>
								<name>factory</name>
								<description>factory</description>
								<displayname>factory</displayname>
								<image>images\urban.png</image>
							</group>   
							<group>
								<name>transportation_line</name>
								<description>transportation_line</description>
								<displayname>transportation_line</displayname>
								<image>images\red_line.png</image>
							</group>	
						</groups>
				    </category>	
				</categories>
            </value>
        </item>
    </layout>
<resources>    
    <resource>
        <type>NETWORK</type>
        <name>transportation_model</name>		       
		<attribute>
            <name>Z</name>
            <dimension>Monetary value</dimension>
            <is_var>Y</is_var>
			<data_type>scalar</data_type>
        </attribute>					
    </resource>      
    <resource>
        <type>NODE</type>
        <name>city</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\reservoir.png</value>
            </item>
            <item>
                <name>group</name>
                <value>city</value>
            </item>
        </layout>      
        <attribute>
            <name>demand</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>
        </attribute>			   
    </resource>      
  <resource>
        <type>NODE</type>
        <name>factory</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\urban.png</value>
            </item>
            <item>
                <name>group</name>
                <value>factory</value>
            </item>
        </layout>      
        <attribute>
            <name>capacity</name>
            <dimension>Dimensionless</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>
        </attribute>			   
    </resource>  	
    <resource>
        <type>LINK</type>
        <name>transportation_line</name>
        <layout>
            <item>
                <name>image</name>
                <value>images\red_line.png</value>
            </item>
			<item>
					<name>symbol</name>
					<value>solid</value>
				</item>
				<item>
					<name>colour</name>
					<value>black</value>
				</item>
				<item>
					<name>line_weight</name>
					<value>1</value>
				</item>   
            <item>
                <name>group</name>
                <value>transportation_line</value>
            </item>
        </layout>
		<attribute>
            <name>trans_cost</name>
            <dimension>Monetary value</dimension>
            <is_var>N</is_var>
			<data_type>scalar</data_type>
        </attribute>
        <attribute>
            <name>shipment_quantities</name>
            <dimension>Dimensionless</dimension>
            <is_var>Y</is_var>
			<data_type>scalar</data_type>
        </attribute>				
    </resource>        	
</resources>
</template_definition>
