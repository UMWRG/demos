<?xml version="1.0"?>
<template_definition>
<template_name>Demo 2 Model</template_name>
<layout>	    
        <item>
            <name>grouping</name>
            <value>			    
			    <name>Demo 2 Model</name>
				<description>Demo 2 network model</description>
				<categories>
				    <category>
						<name>Resources</name>
						<description>Network resources</description>
						<displayname>Network resources</displayname>
						<groups>
							<group>
								<name>supply</name>
								<description>Supply</description>
								<displayname>Supply</displayname>
								<image>images\reservoir.png</image>
							</group>
                            <group>
								<name>demand</name>
								<description>Demand</description>
								<displayname>Demand</displayname>
								<image>images\urban.png</image>
							</group>
                            <group>
								<name>dummy</name>
								<description>Dummy</description>
								<displayname>Dummy</displayname>		
								<image>images\discharge.png</image>
							</group>
						</groups>
				    </category>	
				</categories>
            </value>
        </item>
    </layout>
<resources>
  <resource>
    <type>NODE</type>
    <name>surface_reservoir</name>
	<layout>
		<item>
			<name>image</name>
			<value>images\reservoir.png</value>
		</item>
		<item>
			<name>symbol</name>
			<value>circle</value>
		</item>
		<item>
			<name>colour</name>
			<value>blue</value>
		</item>
        <item>
            <name>group</name>
            <value>supply</value>
        </item>
	</layout>
	<attribute>
      <name>inflow</name>
      <dimension>Volumetric flow rate</dimension>
      <is_var>N</is_var>
	  <data_type>timeseries</data_type>
    </attribute>
    <attribute>
      <name>storagelower</name>
      <dimension>Volume</dimension>
      <is_var>N</is_var>
	  <data_type>timeseries</data_type>
    </attribute>
    <attribute>
      <name>storageupper</name>
      <dimension>Volume</dimension>
      <is_var>N</is_var>
	  <data_type>timeseries</data_type>
    </attribute>
    <attribute>
      <name>initial_storage</name>
      <dimension>Volume</dimension>
      <is_var>N</is_var>
	  <data_type>scalar</data_type>
    </attribute>
    <attribute>
      <name>S</name>
      <dimension>Volumetric flow rate</dimension>
      <is_var>Y</is_var>
	  <data_type>timeseries</data_type>
    </attribute>
  </resource>
  <resource>
    <type>NODE</type>
    <name>urban</name>
    <layout>
        <item>
            <name>image</name>
            <value>images\urban.png</value>
        </item>
        <item>
            <name>symbol</name>
            <value>square</value>
        </item>
        <item>
            <name>colour</name>
            <value>green</value>
        </item>
        <item>
            <name>group</name>
            <value>demand</value>
        </item>
    </layout>
    <attribute>
      <name>delivery</name>
      <dimension>Volumetric flow rate</dimension>
      <is_var>Y</is_var>
      <data_type>timeseries</data_type>
    </attribute>
    <attribute>
      <name>consumption_coefficient</name>
      <dimension>Dimensionless</dimension>
      <is_var>N</is_var>
      <data_type>scalar</data_type>
    </attribute>
  </resource>
  <resource>
    <type>NODE</type>
    <name>agricultural</name>
	<layout>
		<item>
			<name>image</name>
			<value>images\agricultural.png</value>
		</item>
		<item>
			<name>symbol</name>
			<value>square</value>
		</item>
		<item>
			<name>colour</name>
			<value>green</value>
		</item>
        <item>
            <name>group</name>
            <value>demand</value>
        </item>
	</layout>
    <attribute>
      <name>delivery</name>
      <dimension>Volumetric flow rate</dimension>
      <is_var>Y</is_var>
	  <data_type>timeseries</data_type>
    </attribute>
    <attribute>
      <name>consumption_coefficient</name>
      <dimension>Dimensionless</dimension>
      <is_var>N</is_var>
	  <data_type>scalar</data_type>
    </attribute>
  </resource>
  <resource>
    <type>NODE</type>
    <name>junction</name>
	<layout>
		<item>
			<name>image</name>
			<value>images\junction.png</value>
		</item>
		<item>
			<name>symbol</name>
			<value>triangle</value>
		</item>
		<item>
			<name>colour</name>
			<value>yellow</value>
		</item>
        <item>
            <name>group</name>
            <value>demand</value>
        </item>
	</layout>
  </resource>
    <resource>
    <type>NODE</type>
    <name>discharge</name>
  <layout>
    <item>
      <name>image</name>
      <value>images\discharge.png</value>
    </item>
    <item>
      <name>symbol</name>
      <value>triangle</value>
    </item>
    <item>
      <name>colour</name>
      <value>black</value>
    </item>
    <item>
        <name>group</name>
        <value>dummy</value>
    </item>
  </layout>
  </resource>
  <resource>
    <type>LINK</type>
    <name>river_section</name>
	<layout>
		<item>
			<name>image</name>
			<value>images\red_line.png</value>
		</item>
		<item>
			<name>symbol</name>
			<value>solid</value>
		</item>
		<item>
			<name>colour</name>
			<value>red</value>
		</item>
		<item>
			<name>line_weight</name>
			<value>3</value>
		</item>
        
	</layout>
    <attribute>
      <name>cost</name>
      <dimension>Monetary value</dimension>
      <is_var>N</is_var>
	  <data_type>timeseries</data_type>
    </attribute>
    <attribute>
      <name>flow_multiplier</name>
      <dimension>Dimensionless</dimension>
      <is_var>N</is_var>
	  <data_type>timeseries</data_type>
    </attribute>
    <attribute>
      <name>min_flow</name>
      <dimension>Volumetric flow rate</dimension>
      <is_var>N</is_var>
	  <data_type>timeseries</data_type>
    </attribute>
    <attribute>
      <name>max_flow</name>
      <dimension>Volumetric flow rate</dimension>
      <is_var>N</is_var>
	  <data_type>timeseries</data_type>
    </attribute>
	<attribute>
      <name>Q</name>
      <dimension>Volumetric flow rate</dimension>
      <is_var>Y</is_var>
	  <data_type>timeseries</data_type>
    </attribute>
  </resource>
  <resource>
    <type>NETWORK</type>
    <name>tutorial_model</name>
    <attribute>
      <name>Z</name>
      <dimension>Monetary value</dimension>
      <is_var>Y</is_var>
    </attribute>
  </resource>
</resources>
</template_definition>

